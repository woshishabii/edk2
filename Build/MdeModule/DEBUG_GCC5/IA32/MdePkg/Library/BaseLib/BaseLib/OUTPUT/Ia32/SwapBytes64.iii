    SECTION .text

;------------------------------------------------------------------------------
; UINT64
; __attribute__((cdecl))
; InternalMathSwapBytes64 (
; UINT64 Operand
; );
;------------------------------------------------------------------------------
global InternalMathSwapBytes64
InternalMathSwapBytes64:
    mov eax, [esp + 8] ; eax <- upper 32 bits
    mov edx, [esp + 4] ; edx <- lower 32 bits
    bswap eax
    bswap edx
    ret
