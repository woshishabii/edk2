DEFAULT REL
SECTION .text

; INTN
;
; __XenHypercall2 (
; void *HypercallAddr,
; INTN Arg1,
; INTN Arg2
; );
global __XenHypercall2
__XenHypercall2:
  push rdi
  push rsi
  ; Copy HypercallAddr to rax
  mov rax, rcx
  ; Copy Arg1 to the register expected by Xen
  mov rdi, rdx
  ; Copy Arg2 to the register expected by Xen
  mov rsi, r8
  ; Call HypercallAddr
  call rax
  pop rsi
  pop rdi
  ret
